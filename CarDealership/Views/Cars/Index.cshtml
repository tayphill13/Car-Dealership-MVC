@using CarDealership.Models;

<h1>Car Dealership</h1>
@if (Model.Count == 0)
{
  <p>There are no Cars in the list.</p>
}
<ul>
  @foreach (Car x in Model)
  {
  <li>@x.MakeModel</li>
  }   
</ul>

<h1>Cars in your price range</h1>
@if (Model.Count == 0)
{
  <p>There are no cars in your price range</p>
}
<ul>
  @foreach (Car y in Model)
  {
  <li>@y.MakeModel</li>
  }
</ul>

<a href="/">Back to Home</a>
<a href="/cars/addCar">Add another car</a>
